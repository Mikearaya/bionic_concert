{
  "name": "newsweek-clone",
  "version": "1.0.0",
  "description": "Clone of newsweek.com Homepage page",
  "main": "stylelint.config.js",
  "dependencies": {},
  "devDependencies": {
    "html5-validator": "^1.2.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended": "^3.0.0"
  },
  "scripts": {
    "stylelint": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "assets/**/*.css": [
      "npx stylelint --fix"
    ],
    "index.html": [
      "npx html5v"
    ],
    "./pages about.html": [
      "npx html5v"
    ],
    "**/*.+(js|jsx|json|css|md|html)": [
      "npx prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikearaya/newsweek-clone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {},
  "homepage": "https://mikearaya.github.io/newsweek-clone#readme"
}
